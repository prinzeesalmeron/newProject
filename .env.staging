# Staging Environment Configuration
# This file should be used for the staging environment ONLY
# DO NOT commit actual credentials to Git

# Environment
NODE_ENV=staging
VITE_APP_ENV=staging

# API URLs
VITE_SUPABASE_URL=https://staging-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-staging-anon-key-here

# Stripe (Test Mode)
VITE_STRIPE_PUBLIC_KEY=pk_test_your-staging-key-here

# Blockchain Network (Sepolia Testnet)
VITE_NETWORK_NAME=sepolia
VITE_CHAIN_ID=11155111
VITE_RPC_URL=https://sepolia.infura.io/v3/your-project-id

# Contract Addresses (Staging - Sepolia)
VITE_PROPERTY_TOKEN_ADDRESS=0x0000000000000000000000000000000000000000
VITE_MARKETPLACE_ADDRESS=0x0000000000000000000000000000000000000000
VITE_GOVERNANCE_ADDRESS=0x0000000000000000000000000000000000000000
VITE_STAKING_ADDRESS=0x0000000000000000000000000000000000000000

# Monitoring (Staging)
VITE_SENTRY_DSN=https://your-staging-sentry-dsn@sentry.io/project-id
VITE_SENTRY_ENVIRONMENT=staging

# Analytics
VITE_MIXPANEL_TOKEN=your-staging-mixpanel-token

# Feature Flags
VITE_ENABLE_MFA=true
VITE_ENABLE_KYC=true
VITE_ENABLE_STAKING=true
VITE_ENABLE_GOVERNANCE=true

# Limits (Lower for staging)
VITE_MAX_TRANSACTION_AMOUNT=10000
VITE_DAILY_TRANSACTION_LIMIT=50000
VITE_RATE_LIMIT_PER_MINUTE=60

# Debug
VITE_DEBUG_MODE=true
VITE_SHOW_TEST_DATA=true
